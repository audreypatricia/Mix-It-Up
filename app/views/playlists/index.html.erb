<h1>Your playlists</h1>

<ul>

<% @playlists.each do |playlist| %>
  <li><%= link_to (playlist.name), tracks_path(playlist.id) %></li>
<% end %>

</ul>

<% @playlists.each do |playlist| %>
  <%= link_to (image_tag playlist.images.first["url"], :class => 'thumb'), tracks_path(playlist.id)%>
<% end %>
